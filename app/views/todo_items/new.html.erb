<h1>Add New Todo Item</h1>

<%= form_for @item, url: todo_list_todo_items_path(@todo_list) do |form| %>
    <fieldset>
        <%= form.label :description %>
        <%= form.text_field :description %>
        <%= field_errors_for @todo_list, :description %>
    </fieldset>
    <fieldset>
        <%= form.submit %>
    </fieldset>
<% end %>