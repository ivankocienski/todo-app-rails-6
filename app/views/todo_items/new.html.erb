<h1>Add New Todo Item</h1>

<%= form_for @item do |form| %>
    <fieldset>
        <%= form.label :description %>
        <%= form.text_field :description %>
    </fieldset>
    <fieldset>
        <%= form.label :todo_list_id %>
        <%= form.text_field :todo_list_id %>
    </fieldset>
    <fieldset>
        <%= form.submit %>
    </fieldset>
<% end %>