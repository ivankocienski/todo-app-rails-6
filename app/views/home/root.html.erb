<h1>Hello, you</h1>

<ul id='aspirations'>
<% @aspirations.each do |aspiration| %>
    <li><%= link_to aspiration.title, aspiration_path(aspiration) %></li>
<% end %>
</ul>

<ul id='todo-lists'>
<% @todo_lists.each do |todo_list| %>
    <li><%= link_to todo_list.title, todo_path(todo_list) %></li>
<% end %>
</ul>

<p>
    A small application for managing things to get done. 
</p>

